<?xml version="1.0" ?>
<gfx-xform version="1">
    <input file="./images/background.png" type="png" tile-width="8" tile-height="8"/>
    <output file="./background.inc">
        <tiles var-name="backgroundTiles"/>
        <maps pointers-size="8">
            <map var-name="map_blank" left="0" top="0" width="1" height="1"/>
            <map var-name="map_enos" left="3" top="1" width="3" height="4"/>
            <map var-name="map_banditAvatar" left="0" top="1" width="3" height="4"/>
            <map var-name="map_terrain" left="6" top="0" width="28" height="20"/>
	</maps>	
    </output>
</gfx-xform>
